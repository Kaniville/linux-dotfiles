ZHOME=$HOME/.config/zsh
ZPLUG=$HOME/.config/zsh/plugins

HISTFILE=$HOME/.zhistory

export TMOUT=300

export QT_QPA_PLATFORMTHEME=qt5ct

export EDITOR=nvim
export VISUAL=nvim
export PAGER=less

export SSH_AUTH_SOCK

export MOZ_DBUS_REMOTE=1
