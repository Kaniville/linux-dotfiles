export BEMENU_OPTS="-w -i -H '21' --fn 'Fira Code Regular 9' --tb '#000000' --tf '#d8dee9' --fb '#000000' --nb '#000000' --nf '#d8dee9' --hb '#000000' --hf '#bf616a'"
